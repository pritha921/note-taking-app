<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Notes</title>
</head>
<body>
    <h1>Your Notes</h1>

    <% if (notes.length > 0) { %>
        <ul>
            <% notes.forEach(note => { %>
                <li><%= note.title %>: <%= note.content %></li>
            <% }); %>
        </ul>
    <% } else { %>
        <p>No notes found.</p>
    <% } %>

    <form action="/dashboard/notes/addNote" method="GET">
        <button type="submit">Add notes</button>
    </form>
    <form action="/logout" method="POST">
        <button type="submit">Logout</button>
    </form>
</body>
</html>

